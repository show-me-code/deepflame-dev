FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Left
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    Right
    {
        type            inletOutlet;
        inletValue      uniform (-487.34 0 0);
        value           uniform (-487.34 0 0);
    }

    empty
    {
        type            empty;
    }
}

